{
  "name": "xisu-course-schedule",
  "version": "1.0.1",
  "private": true,
  "description": "基于Taro的西安外国语大学课程表小程序",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "audit:check": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf dist node_modules/.cache",
    "reinstall": "rm -rf node_modules package-lock.json && npm install"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "西安外国语大学",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons-svg": "^4.4.2",
    "@babel/runtime": "^7.24.0",
  "@tarojs/components": "^4.1.7",
  "@tarojs/helper": "^4.1.7",
  "@tarojs/plugin-framework-react": "^4.1.7",
  "@tarojs/plugin-platform-alipay": "^4.1.7",
  "@tarojs/plugin-platform-h5": "^4.1.7",
  "@tarojs/plugin-platform-jd": "^4.1.7",
  "@tarojs/plugin-platform-qq": "^4.1.7",
  "@tarojs/plugin-platform-swan": "^4.1.7",
  "@tarojs/plugin-platform-tt": "^4.1.7",
  "@tarojs/plugin-platform-weapp": "^4.1.7",
  "@tarojs/react": "^4.1.7",
  "@tarojs/runtime": "^4.1.7",
  "@tarojs/shared": "^4.1.7",
  "@tarojs/taro": "^4.1.7",
    "lucide-react": "^0.544.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
  "@tarojs/cli": "^4.1.7",
  "@tarojs/webpack5-runner": "^4.1.7",
    "@types/react": "^18.2.0",
    "@types/webpack-env": "^1.18.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
  "babel-preset-taro": "^4.1.7",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
  "eslint-config-taro": "^4.1.7",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.35",
    "react-refresh": "^0.14.0",
    "sass": "^1.71.0",
    "sass-loader": "^14.1.0",
    "style-loader": "^3.3.4",
    "typescript": "^5.3.3",
    "webpack": "^5.94.0"
  },
  "overrides": {
    "postcss": "^8.4.35",
    "webpack": "^5.94.0",
    "esbuild": "^0.21.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
